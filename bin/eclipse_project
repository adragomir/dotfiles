#!/bin/bash

from=$1
to=$2

to_move=( .settings .externalToolBuilders .eclipse.templates .launches .classpath .project )
to_move_type=( d d d d f f )
for idx in $(seq 0 $((${#to_move[@]} - 1))); do
  t=${to_move[$idx]}
  tf=${to_move_type[$idx]}
  echo "Moving $t..."

  for d in `find $from \( -iname "$t" -a -type $tf \)`; do
    df=$(dirname $d)
    fc=${df#$from/}
    mkdir -p $to/$fc/
    echo mv $d $to/$fc/
    mv $d $to/$fc/
  done
  
done

